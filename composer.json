{
    "name": "martynbiz/slim-modules-app",
    "description": "Slim modules skeleton app",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "url": "https://github.com/martynbiz/slim-module.git",
            "type": "git"
        },
	    {
            "url": "https://github.com/martynbiz/slim-module-core.git",
            "type": "git"
        },
	    {
            "url": "https://github.com/martynbiz/slim-module-auth.git",
            "type": "git"
        },
	    {
            "url": "https://github.com/martynbiz/slim-module-register.git",
            "type": "git"
        },
	    {
            "url": "https://github.com/martynbiz/slim-module-blog.git",
            "type": "git"
        },
	    {
            "url": "https://github.com/martynbiz/php-events.git",
            "type": "git"
        },
	    {
            "url": "https://github.com/martynbiz/php-translate-tool.git",
            "type": "git"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "slim/slim": "^3.1",
        "martynbiz/slim-module": "dev-master",
        "martynbiz/slim-module-core": "dev-master",
        "martynbiz/slim-module-auth": "dev-master",
        "martynbiz/slim-module-register": "dev-master",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.*",
        "symfony/dom-crawler": "^3.2",
        "symfony/css-selector": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/modules/app/src/",
            "Tests\\": "tests/"
        }
    }
}
